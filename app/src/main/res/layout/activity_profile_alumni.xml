<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile.ProfileTeacher">

    <TextView
        android:id="@+id/alumniProfileUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="25dp"
        android:text="Username"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/alumniProfilePhoto"
        android:layout_width="125dp"
        android:layout_height="123dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/ic_baseline_person_pin_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alumniProfileUsername" />

    <TextView
        android:id="@+id/alumniProfileName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfileUsername"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="184dp"
        android:layout_toEndOf="@+id/alumniProfilePhoto"
        android:text="Name"
        android:textSize="21dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/EditProfileButton"
        android:layout_width="223dp"
        android:layout_height="50dp"
        android:layout_below="@id/alumniProfileName"
        android:layout_margin="20dp"
        android:layout_toEndOf="@+id/alumniProfilePhoto"
        android:text="Edit Profile"
        android:textColor="@android:color/white"
        android:background="@drawable/button_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="147dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/alumniProfileBranch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfileBatchFill"
        android:padding="10dp"
        android:text="Branch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteY="238dp" />

    <TextView
        android:id="@+id/alumniProfileBranchFill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfileBranch"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:padding="10dp"
        android:text="Name of branch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alumniProfileBranch"
        app:layout_constraintTop_toBottomOf="@+id/alumniProfilePhoto" />

    <TextView
        android:id="@+id/alumniProfileBatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfilePhoto"
        android:padding="10dp"
        android:text="Batch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alumniProfileBranch"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/alumniProfileBatchFill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfileBatch"
        android:layout_marginTop="8dp"
        android:padding="10dp"
        android:text="Batch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alumniProfileBatch"
        app:layout_constraintTop_toBottomOf="@+id/alumniProfileBranchFill" />

    <TextView
        android:id="@+id/alumniProfileTeams"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfileBranchFill"
        android:padding="10dp"
        android:text="Teams"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alumniProfileBatch"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/alumniProfileTeamsFill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alumniProfileTeams"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:padding="10dp"
        android:text="None"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alumniProfileTeams"
        app:layout_constraintTop_toBottomOf="@+id/alumniProfileBatchFill" />
</androidx.constraintlayout.widget.ConstraintLayout>